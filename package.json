{
  "name": "aoc2024",
  "version": "1.0.0",
  "scripts": {
    "solve": "ts-node --transpile-only tools/solve/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "generate": "ts-node --transpile-only tools/initialize_challenge/index.ts"
  },
  "keywords": [
    "advent-of-code",
    "typescript",
    "aoc2024"
  ],
  "author": "Alex Szabo",
  "license": "CC-BY-NC-4.0",
  "description": "Advent of Code 2024 Solutions in TypeScript",
  "devDependencies": {
    "@inquirer/prompts": "^3.3.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
